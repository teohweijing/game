<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="control-group">
    <%= f.email_field :email, placeholder: "email", class: "login-field" %>
  </div>
  <div class="control-group">
    <%= f.password_field :password, autocomplete: "off", placeholder: "password", class: "login-field" %>
  </div>
  <div class="actions">
    <%= f.submit "login", class: "btn btn-primary btn-large btn-block" %>
  </div>
<% end %>
